<body class="background_image_sign_up">

    <div class="container">
        <div class="row pt-115">
            <div class="col-md-4"></div>




            <div class="col-md-4">
                <div class="card">
                    <div class="card-body">
                        <form id="demo-form2" action="@Url.Action("Login", "Home")" method="post" data-parsley-validate class="" enctype="multipart/form-data">
                            <h3 class="box-title m-b-20">Sign In</h3>
                            <div class="form-group ">
                                <div class="col-xs-12">
                                    <input class="form-control bg_white p-l-10" type="email" required="" placeholder="Email" name="email">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-xs-12">
                                    <input class="form-control bg_white p-l-10" type="password" required="" placeholder="Password" name="password">
                                </div>
                            </div>

                            <div class="form-group text-center m-t-20">
                                <div class="col-xs-12">
                                    <button class="btn btn-info btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">Log In</button>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 m-t-10 text-center">
                                    <div class="social">
                                        <a href="https://www.facebook.com/" target="_blank" class="btn  btn-facebook" data-toggle="tooltip"> <i aria-hidden="true" class="fa fa-facebook"></i> </a>
                                        <a href="https://www.gmail.com/" target="_blank" class="btn btn-googleplus" data-toggle="tooltip"> <i aria-hidden="true" class="fa fa-google-plus"></i> </a>
                                        <a href="https://twitter.com/" target="_blank" class="btn btn-twitter" data-toggle="tooltip"> <i aria-hidden="true" class="fa fa-twitter"></i> </a>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group p-t-10 m-b-0">
                                <div class="col-sm-12 text-center">
                                    <p>Don't have an account? <a href="~/Admin/Sign_Up" class="text-info m-l-5"><b>Sign Up</b></a></p>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>


            <div class="col-md-4"></div>
        </div>


        <div class="row">
            
        </div>

        

    </div>

   

    <script type="text/javascript">
    var count = 0; // needed for safari
    window.onload = function () {

        if (typeof history.pushState === "function") {
            history.pushState("back", null, null);
            window.onpopstate = function () {
                //alert();
                history.pushState('back', null, null);
                //if(count == 1){window.location.href = '{{ url(route_name() . '/all-hospitals') }}';}
                //window.location.href = '{{ url(route_name() . '/all-hospitals' ) }}'
            };
        }
    }
    setTimeout(function(){count = 1;},200);
    </script>

</body>